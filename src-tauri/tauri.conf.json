{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "repairgest",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "repairgest",
      "icon": [
        "icons/icon.icns",
        "icons/icon.ico",
        "icons/logo190x170.png",
        "icons/logo32x28.png"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "RepairGest",
        "width": 800,
        "minWidth": 400,
        "height": 600,
        "minHeight": 400
      }
    ]
  }
}
